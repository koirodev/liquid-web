{
  "name": "liquid-web",
  "version": "@version",
  "description": "@description",
  "type": "module",
  "main": "./liquid-core.mjs",
  "module": "./liquid-core.mjs",

  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./liquid-core.mjs"
    },

    "./modules": {
